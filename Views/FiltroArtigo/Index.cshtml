@using NParty.Www.Models
@model List<Article>
@{
    ViewBag.Title = "Index";
    Layout = null;
}

<table cellspacing="2">
    <thead>
        <tr>
            <th>Título</th>
            <th>Data</th>
            <th>Link</th>
        </tr>
    </thead>
    @foreach(Article a in Model){
        <tr>
            <td>@a.Title</td>
            <td>@a.DatePublished.ToString("dd/MM/yyyy")</td>
            <td>http://nparty-www.azurewebsites.net/@a.NPartyArticleLink</td>
        </tr>
    }
    <tr></tr>
</table>
